{
  "overrides": [
    {
      "files": ["*.ts", "*.vue", "*.tsx"],
      "extends": "@antfu",
      "rules": {
        "no-console": "off",
        "prefer-rest-params": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "extends": "alloy",
      "parser": "babel-eslint"
    }
  ]
}
